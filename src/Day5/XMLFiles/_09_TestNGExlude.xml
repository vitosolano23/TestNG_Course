<!-- Exclude keyword will run all the @Test except the ones with "SmokeTest" group! -->

<suite name="SmokeTest Suite">

    <groups>
        <run>
            <exclude name="SmokeTest"></exclude>
        </run>
    </groups>

    <test name="SmokeTest">
        <classes>
            <class name="Day5._03_Groups"></class>
            <class name="Day5._04_SecondGroups"></class>
        </classes>
    </test>

</suite>